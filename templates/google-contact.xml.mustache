<atom:entry xmlns:atom='http://www.w3.org/2005/Atom'
        xmlns:gd='http://schemas.google.com/g/2005'
        xmlns:gContact='http://schemas.google.com/contact/2008'>
      <atom:category scheme='http://schemas.google.com/g/2005#kind'
        term='http://schemas.google.com/contact/2008#contact'/>
	<gd:name>
		<gd:givenName>{{prenom}}</gd:givenName>
		<gd:familyName>{{nom}}</gd:familyName>
	</gd:name>
	<gd:organization rel="http://schemas.google.com/g/2005#work" primary="true">
		<gd:orgName>{{association}}</gd:orgName>
		<gd:orgTitle>Responsable sweats</gd:orgTitle>
	</gd:organization>
	<atom:content type='text'>Contact ajouté automatiquement par le site de devis de School Phenomenon, créé par Ophir LOJKINE.</atom:content>
	<gd:email rel='http://schemas.google.com/g/2005#work' primary='true' address="{{email}}" />
	<gContact:birthday when='{{date-naissance-ymd}}'/>
	<gd:phoneNumber rel='http://schemas.google.com/g/2005#work' primary='true'>
		{{telephone}}
	</gd:phoneNumber>
	<gd:structuredPostalAddress rel='http://schemas.google.com/g/2005#work' primary='true'>
		<gd:city>{{ville}}</gd:city>
		<gd:street>{{adresse}}</gd:street>
		<gd:postcode>{{code-postal}}</gd:postcode>
		<gd:country>France</gd:country>
	</gd:structuredPostalAddress>
	<gContact:groupMembershipInfo deleted='false' href='{{gcontact-group-id}}'/>
	</atom:entry>
